<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Mission Appossible</title>
        <link href="./css/main.css" rel="stylesheet">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        <header>
            <div class="app_bar">
                <img class="menu_button" src="./img/menu.png" alt="Menu" onclick="menu_function()">
                <img class="search" src="./img/search.png" alt="Search">
                <img class="navigation" src="./img/navigation.png" alt="Navigation">
            </div>
           
            <div id="dark_box" class="dark_box" onclick="menu_function()">
            </div>
            <nav id="menu" class="menu">
                <img class="menu_img" src="./img/menu_img.jpg">
                <img class="account_img" src="./img/account_img.png">

                <div class="menu_options">
                    <div class="starred">
                        <a href="index.html">
                            <img src="./img/star_orange.png" class="menu_icon">
                            Starred
                        </a>
                    </div>
                    </div class="new">
                    </div>
                    <div class="settings">
                        <a href="settings.html">Settings</a>
                    </div>
                </div>
            </nav>
        </header>
        <%= yield %>
    </body>
    <script defer="defer" src="./js/script.js"></script>
</html>